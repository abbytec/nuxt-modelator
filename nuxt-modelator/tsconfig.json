{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"declaration": true,
		"outDir": "dist",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"types": ["node", "@nuxt/schema"]
	},
	"include": ["src"]
}
